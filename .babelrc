{
  "presets": ["@wordpress/babel-preset-default"],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "wp.element.createElement"
      }
    ]
  ],
  "env": {
    "default": {
      "plugins": [
        "transform-runtime"
      ]
    }
  }
}
