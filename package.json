{
  "name": "connections",
  "title": "Connections Business Directory",
  "version": "8.30.1",
  "description": "Business directory plugin for WordPress.",
  "homepage": "https://connections-pro.com/",
  "author": {
    "name": "Steven A Zahm",
    "email": "",
    "url": "http://connections-pro.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Connections-Business-Directory/Connections"
  },
  "bugs": {
    "url": "https://github.com/Connections-Business-Directory/Connections/issues",
    "email": ""
  },
  "license": "GPLv2+",
  "private": true,
  "config": {
    "makepot": {
      "type": "wp-plugin",
      "domain": "connections",
      "src": "connections.php",
      "dest": "languages/",
      "header": {
        "bugs": "http://connections-pro.com/support/forum/translations/",
        "team": "WP-Translations (http://wp-translations.org/)",
        "last_translator": "WP-Translations <wpt@wp-translations.org>",
        "language": "en_US"
      }
    },
    "uglify": {
      "core": {
        "src": "assets/js/",
        "dest": "assets/js/"
      }
    }
  },
  "scripts": {
    "start": "npm install && npm run watch",
    "dev": "cross-env webpack",
    "production": "cross-env NODE_ENV=production webpack",
    "build": "npm run production",
    "watch": "cross-env webpack --watch",
    "lint": "npm run lint:js & npm run lint:scss",
    "lint:js": "eslint assets/src/**/*.js blocks/**/*.js",
    "lint:scss": "stylelint assets/src/**/*.scss"
  },
  "dependencies": {
    "npm": "^6.4.1",
    "query-string": "^5.1.1",
    "script-loader": "^0.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.6",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "3.2.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-react": "^7.10.0",
    "expose-loader": "latest",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "grunt": "^1.0.3",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^2.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-exec": "~3.0.0",
    "grunt-potomo": "~3.5.0",
    "grunt-wp-i18n": "~1.0.1",
    "grunt-wp-readme-to-markdown": "^2.0.1",
    "imagemin-webpack-plugin": "^2.3.0",
    "load-grunt-tasks": "^3.1.0",
    "logfile-grunt": "^0.4.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.6",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "stylelint": "^9.8.0",
    "stylelint-config-wordpress": "^12.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.1.2",
    "webpack-rtl-plugin": "^1.7.0",
    "wp-pot": "latest"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  }
}
