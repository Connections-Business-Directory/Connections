{
  "name": "connections",
  "version": "8.5.30",
  "description": "Connections Business Directory plugin for WordPress.",
  "homepage": "http://connections-pro.com/",
  "author": {
    "name": "Steven A Zahm",
    "email": "",
    "url": "http://connections-pro.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Connections-Business-Directory/Connections"
  },
  "bugs": {
    "url": "https://github.com/Connections-Business-Directory/Connections/issues",
    "email": ""
  },
  "license": "GPLv2+",
  "private": true,
  "config": {
    "makepot": {
      "type": "wp-plugin",
      "domain": "connections",
      "src": "connections.php",
      "dest": "languages/",
      "header": {
        "bugs": "http://connections-pro.com/support/forum/translations/",
        "team": "WP-Translations (http://wp-translations.org/)",
        "last_translator": "WP-Translations <wpt@wp-translations.org>",
        "language": "en_US"
      }
    },
    "uglify": {
      "core": {
        "src": "assets/js/",
        "dest": "assets/js/"
      }
    }
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "^2.2.0",
    "grunt-checktextdomain": "^0.1.1",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-csslint": "^0.4.0",
    "grunt-contrib-cssmin": "^0.12.2",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.8.0",
    "grunt-exec": "~0.4.5",
    "grunt-phpdocumentor": "^0.4.1",
    "grunt-potomo": "~3.2.1",
    "grunt-wp-i18n": "~0.5.0",
    "grunt-wp-readme-to-markdown": "^1.0.0",
    "load-grunt-tasks": "^3.1.0",
    "logfile-grunt": "^0.1.6"
  }
}
