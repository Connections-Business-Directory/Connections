{
  "name": "connections",
  "title": "Connections Business Directory",
  "version": "9.4.1",
  "description": "Business directory plugin for WordPress.",
  "homepage": "https://connections-pro.com/",
  "author": {
    "name": "Steven A Zahm",
    "email": "",
    "url": "https://connections-pro.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Connections-Business-Directory/Connections"
  },
  "bugs": {
    "url": "https://github.com/Connections-Business-Directory/Connections/issues",
    "email": ""
  },
  "license": "GPLv2+",
  "private": true,
  "config": {
    "makepot": {
      "type": "wp-plugin",
      "domain": "connections",
      "src": "connections.php",
      "dest": "languages/",
      "header": {
        "bugs": "http://connections-pro.com/support/forum/translations/",
        "team": "WP-Translations (http://wp-translations.org/)",
        "last_translator": "WP-Translations <wpt@wp-translations.org>",
        "language": "en_US"
      }
    },
    "uglify": {
      "core": {
        "src": "assets/js/",
        "dest": "assets/js/"
      }
    }
  },
  "engines": {
    "node": "8.9.1",
    "npm": "6.12.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/recommended"
    ]
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 1 Android versions",
    "last 1 ChromeAndroid versions",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions"
  ],
  "dependencies": {
    "@fonticonpicker/fonticonpicker": "^3.1.1",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "chosen-js": "^1.8.7",
    "leaflet": "^1.6.0",
    "picturefill": "^3.0.3",
    "pure-react-carousel": "^1.24.1",
    "query-string": "^5.1.1",
    "react-slick": "^0.25.2",
    "script-loader": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@wordpress/babel-preset-default": "3.0.2",
    "@wordpress/browserslist-config": "2.2.3",
    "@wordpress/eslint-plugin": "1.0.1",
    "autoprefixer": "^9.7.3",
    "babel-loader": "8.0.5",
    "babel-preset-env": "^1.7.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "5.2.0",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "expose-loader": "latest",
    "file-loader": "^1.1.11",
    "grunt": "^1.0.4",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^2.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-exec": "~3.0.0",
    "grunt-potomo": "~3.5.0",
    "grunt-wp-i18n": "^1.0.3",
    "grunt-wp-readme-to-markdown": "^2.0.1",
