    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.25",
    "postcss-loader": "^2.1.6",
    "react-select": "^2.4.4",
    "sass-loader": "^6.0.7",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.18.2",
    "stylelint": "^9.10.1",
